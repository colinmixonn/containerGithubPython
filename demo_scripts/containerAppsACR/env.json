{
    "RESOURCE_GROUP": "my-container-apps",
    "LOCATION": "eastus",
    "CONTAINERAPPS_ENVIRONMENT":"my-environment",
    "CONTAINER_APP_NAME": "my-container-app",
    "SUBSCRIPTION_ID"=$(az account show --query id --out tsv) SCOPE="/subscriptions/${SUBSCRIPTION_ID}/resourceGroups/${RESOURCE_GROUP}" [[ -z "${RANDOM_STR:-}" ]] && RANDOM_STR=$(echo -n "$SCOPE" | shasum | head -c 6),
    "ACR_NAME":"acr${RANDOM_STR}",
    "ACR_IMAGE_NAME":"pypy-fastapi:latest"
}
